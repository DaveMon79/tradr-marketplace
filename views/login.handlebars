<div class="bg-col-white">
    <div class="container text-center">
        <div class="row parent-row">
                <!---log in page-->
                <div class="login-parent">
                    <div class="col-10 product-container-dashboard">
                        <div class="row active-listing">
                            <p class="active-listing-title">LOG IN</p>
                            <hr>
                        </div>
                        <form id="login-form">
                        <div class="row">
                            <div class="col input-form">
                                <label for="inputEmail" class="email-header-text">Email address:</label>
                                <input type="email" class="form-control" id="inputEmail" placeholder="Enter email">
                                <label for="inputPassword" class="password-header-text">Password:</label>
                                <input type="password" class="form-control" id="inputPassword" placeholder="Enter password">
                            </div>
                        </div>
                        <div class="row btn-row">
                            <hr>
                            <button id="login" type="submit" class="btn btn-dark btn-styling login-btn">LOG IN</button>
                        </div>
                        </form>
                    </div>
                </div>
                   <!---log in page end-->
        </div>
    </div>
</div>

<script src="/js/login.js"></script>